
<div class="flickr">
  <ul>
    <% photos.each do |row| %>
      <li>
      <% row.each do |p| %>
        <% if !p.nil? %>
          <% info = flickr.photos.getInfo(photo_id: p.id) %> 
          <%= link_to(image_tag(FlickRaw.url_q(info), :class => 'flickr_photo', :title => p.title, :border => 0), FlickRaw.url_photopage(info)) %>  

        <% end %>
        <% end %>
      </li>
    <% end %>
  </ul>

</div>
